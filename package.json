{
  "name": "yapping-level",
  "version": "2.0.0",
  "description": "Analyzes historical and current level of discord chatter to calculate the current level of yapping going on.",
  "main": "index.js",
  "scripts": {
    "start": "dotenvx run -- node yapping-level.js",
    "server": "dotenvx run -- pm2 start yapping-level.js",
    "deploy:dev": "dotenvx run -- node deployscripts/dev.js",
    "deploy:global" : "dotenvx run -- node deployscripts/global.js",
    "delete:dev": "dotenvx run -- node deployscripts/deleteDev.js",
    "delete:global": "dotenvx run -- node deployscripts/deleteGlobal.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aldoraz/Yapping-Level.git"
  },
  "keywords": [
    "discord.js"
  ],
  "author": "Aldoraz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aldoraz/Yapping-Level/issues"
  },
  "homepage": "https://github.com/Aldoraz/Yapping-Level#readme",
  "dependencies": {
    "cli-color": "^2.0.4",
    "discord.js": "^14.15.2",
    "pg": "^8.11.5"
  }
}
